{
  "checkstyle": {
    "prefix": "checkstyle",
    "body": ["// CHECKSTYLE OFF: $0", ""],
    "description": "Checkstyle suppression"
  },
  "comment": {
    "prefix": "comment",
    "body": ["/*", " * $1", " */", "$0"],
    "description": "Comment"
  },
  "javadoc": {
    "prefix": "javadoc",
    "body": ["/**", " * $1", " */", "$0"],
    "description": "Javadoc comment"
  },
  "systemout": {
    "prefix": "sout",
    "body": "System.out.println($1);$0",
    "description": "Print to System.out"
  },
  "systemoutlong": {
    "prefix": "sysout",
    "body": "System.out.println($1);$0",
    "description": "Print to System.out"
  },
  "systemerr": {
    "prefix": "syserr",
    "body": "System.err.println($1);$0",
    "description": "Print to System.err"
  },
  "test": {
    "prefix": "test",
    "body": ["@Test", "${1|void,public void|} $2() {", "\t$0", "}"],
    "description": "JUnit test case"
  },
  "assertEquals": {
    "prefix": "assertEquals",
    "body": "assertEquals($1, $2);$0",
    "description": "assert that two values are equal"
  }
}
