{
  "issue": {
    "prefix": "issue",
    "body": "([Issue $1](https://github.com/jqno/equalsverifier/issues/$1)$2)$0",
    "description": "EqualsVerifier issue reference"
  },
  "nowrap": {
    "prefix": "nowrap",
    "body": "%% vim:nowrap",
    "description": "Vim modeline to disable wrapping in this file"
  },
  "prettier-ignore": {
    "prefix": "prettier-ignore",
    "body": ["<!-- prettier-ignore-start -->", "$0", "<!-- prettier-ignore-end -->"],
    "description": "Block which will not be formatted"
  }
}
