partial modifier_keys
xkb_symbols "swap_alt_super" {
    replace key <LWIN> { [ Alt_L, Alt_L ] };
    replace key <LALT> { [ Super_L, Super_L ] };
    replace key <RWIN> { [ Alt_R, Alt_R ] };
    replace key <RALT> { [ Super_R, Super_R ] };
};

partial alphanumeric_keys
xkb_symbols "compose" {
    replace key <RWIN> { [ Multi_key ] };
};

partial alphanumeric_keys
xkb_symbols "tilde" {
    replace key <LSGT> { [ grave, tilde ] };
};
